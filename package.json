{
  "name": "diegins",
  "version": "0.0.1",
  "description": "Diegins",
  "main": "app.js",
  "author": {
    "name": "Diego Di Mauro",
    "url": "https://github.com/hurrikam"
  },
  "scripts": {
    "lint": "./node_modules/.bin/tslint './src/server/**/*.ts'",
    "compile": "./node_modules/.bin/tsc",
    "package": "./node_modules/.bin/webpack",
    "build": "npm run compile && npm run package",
    "build-dev": "npm run compile && npm run package -- --mode=development",
    "start": "node ./output/server/app.js",
    "test": "npm run lint && npm run unit-test",
    "unit-test": "npm run compile && jest ./output --testPathPattern='/.*\\.spec.js$'"
  },
  "dependencies": {
    "@reach/router": "1.2.1",
    "axios": "0.18.0",
    "express": "4.16.4",
    "react": "16.6.1",
    "react-dom": "16.6.1",
    "typescript": "3.5.3",
    "webpack": "4.25.1",
    "webpack-cli": "3.1.2"
  },
  "devDependencies": {
    "@types/axios": "0.14.0",
    "@types/express": "4.16.0",
    "@types/jest": "23.3.5",
    "@types/reach__router": "1.2.4",
    "@types/react": "16.7.2",
    "@types/react-dom": "16.0.9",
    "jest": "23.2.0",
    "tslint": "5.11.0",
    "tslint-eslint-rules": "5.4.0"
  }
}
